{
  "family": "fracktail-task-definition",
  "executionRoleArn": "arn:aws:iam::379634893371:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "fracktail",
      "image": "jlamanna/fracktail:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": true,
          "awslogs-group": "/ecs/fracktail-task-definition",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 8080,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "cpu": 0,
      "secrets": [
        {
          "name": "discord.token",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:379634893371:secret:fracktail-YZVN8F"
        }
      ],
      "memory": 400,
      "stopTimeout": 30
    }
  ],
  "memory": "2048",
  "compatibilities": [
    "EC2",
    "FARGATE"
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "512"
}